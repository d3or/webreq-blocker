{
  "manifest_version": 2,
  "name": "blocker",
  "description": "block urls!",
  "version": "1.0",
  "permissions": ["<all_urls>", "storage", "webRequest", "webRequestBlocking"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
